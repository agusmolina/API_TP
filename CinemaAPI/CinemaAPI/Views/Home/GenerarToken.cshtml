@using CinemaAPI.Models;
@model TokenResponse
@{
    ViewBag.Title = "Contact";
}


<div class="row  justify-content-center" style="padding-top:10px;">
    <div class="col-6 justify-content-center" style="padding: 20px 20px !important; background-color:white; border-radius:10px" >
        <h4 style=" padding-top:5px; padding-bottom:10px;font-weight:bold; font-family: 'Exo', sans-serif;text-align:center">TOKEN AUTENTICACION</h4>
        <form method="post" style="margin-left:130px; margin-bottom:30px" action="@Url.Action("ResultadoToken", "Home")">
        <div class="form-group">
            <label for="midni">Dni</label>
            <input type="text" class="form-control" id="midni" placeholder="Ingresa Dni" name="dni">
        </div>
        <div class="form-group">
            <label for="Contrasena">Contrasena</label>
            <input type="password" class="form-control" id="password" placeholder="Password" name="Contrasena">
        </div>
        <button type="submit" class="btn btn-primary">Generar Token</button>
        </form>
    </div>
</div>
